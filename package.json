{
  "name": "dana-v3",
  "version": "1.0.0",
  "description": "Personal website built on Eleventy and webpack.",
  "homepage": "https://github.com/dananjohnson/dana-v3",
  "main": "index.js",
  "author": {
    "name": "Dana Johnson"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dananjohnson/dana-v3"
  },
  "license": "CC-BY-4.0",
  "browserslist": {
    "production": [
      "> 1%",
      "last 2 versions",
      "Firefox ESR",
      "not dead",
      "not ie 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "clean": "rimraf dist",
    "start": "npm-run-all -l clean --parallel start:*",
    "start:webpack": "NODE_ENV=development webpack --watch",
    "start:eleventy": "NODE_ENV=development eleventy --serve --quiet",
    "build": "npm-run-all -l clean build:*",
    "build:webpack": "NODE_ENV=production webpack",
    "build:eleventy": "NODE_ENV=production eleventy"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-img": "^1.0.0",
    "@11ty/eleventy-plugin-rss": "^1.1.2",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.1.3",
    "browserslist": "^4.19.1",
    "css-loader": "^6.5.1",
    "eleventy-plugin-error-overlay": "^0.1.2",
    "luxon": "^2.2.0",
    "markdown-it-attrs": "^4.1.1",
    "markdown-it-bracketed-spans": "^1.0.1",
    "mini-css-extract-plugin": "^2.4.5",
    "npm-run-all": "^4.1.5",
    "nunjucks": "^3.2.3",
    "postcss": "^8.4.5",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^6.2.1",
    "postcss-preset-env": "^7.0.2",
    "prettier": "^2.5.1",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-manifest-plugin": "^4.0.2"
  }
}
