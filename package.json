{
  "name": "dana-v3",
  "version": "1.0.0",
  "description": "Personal website built on Eleventy and webpack.",
  "homepage": "https://github.com/dananjohnson/dana-v3",
  "main": "index.js",
  "author": {
    "name": "Dana Johnson"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dananjohnson/dana-v3"
  },
  "license": "CC-BY-4.0",
  "engines": {
    "node": "20.x"
  },
  "browserslist": {
    "production": [
      "> 1%",
      "last 2 versions",
      "Firefox ESR",
      "not dead",
      "not ie 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "clean": "rimraf dist",
    "start": "npm-run-all -l clean --parallel start:*",
    "start:webpack": "NODE_ENV=development webpack --watch",
    "start:eleventy": "NODE_ENV=development eleventy --serve --quiet",
    "build": "npm-run-all -l clean build:*",
    "build:webpack": "NODE_ENV=production webpack",
    "build:eleventy": "NODE_ENV=production eleventy",
    "browserslist:update": "npx update-browserslist-db@latest"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-img": "^4.0.2",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "browserslist": "^4.23.0",
    "css-loader": "^6.10.0",
    "luxon": "^3.4.4",
    "markdown-it": "^14.0.0",
    "markdown-it-attrs": "^4.1.6",
    "markdown-it-bracketed-spans": "^1.0.1",
    "markdown-it-footnote": "^4.0.0",
    "mini-css-extract-plugin": "^2.8.0",
    "npm-run-all": "^4.1.5",
    "nunjucks": "^3.2.4",
    "postcss": "^8.4.35",
    "postcss-import": "^16.0.1",
    "postcss-loader": "^8.1.0",
    "postcss-preset-env": "^9.4.0",
    "prettier": "^2.8.8",
    "sharp": "^0.33.2",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4",
    "webpack-manifest-plugin": "^5.0.0"
  }
}
