{
  "name": "dana-v3",
  "version": "1.0.0",
  "description": "Personal website built on Eleventy and webpack.",
  "homepage": "https://github.com/dananjohnson/dana-v3",
  "main": "index.js",
  "author": {
    "name": "Dana Johnson"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dananjohnson/dana-v3"
  },
  "license": "CC-BY-4.0",
  "browserslist": {
    "production": [
      "> 1%",
      "last 2 versions",
      "Firefox ESR",
      "not dead",
      "not ie 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "clean": "rimraf dist",
    "start": "npm-run-all -l clean --parallel start:*",
    "start:webpack": "NODE_ENV=development webpack --watch",
    "start:eleventy": "NODE_ENV=development eleventy --serve --quiet",
    "build": "npm-run-all -l clean build:*",
    "build:webpack": "NODE_ENV=production webpack",
    "build:eleventy": "NODE_ENV=production eleventy",
    "browserslist:update": "npx browserslist@latest --update-db"
  },
  "devDependencies": {
    "@11ty/eleventy": "^1.0.2",
    "@11ty/eleventy-img": "^2.0.1",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.2.0",
    "browserslist": "^4.21.4",
    "css-loader": "^6.7.3",
    "eleventy-plugin-error-overlay": "^1.0.0",
    "luxon": "^3.2.1",
    "markdown-it-attrs": "^4.1.6",
    "markdown-it-bracketed-spans": "^1.0.1",
    "markdown-it-footnote": "^3.0.3",
    "mini-css-extract-plugin": "^2.7.2",
    "npm-run-all": "^4.1.5",
    "nunjucks": "^3.2.3",
    "postcss": "^8.4.21",
    "postcss-import": "^15.1.0",
    "postcss-loader": "^7.0.2",
    "postcss-preset-env": "^8.0.0",
    "prettier": "^2.8.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-manifest-plugin": "^5.0.0"
  },
  "dependencies": {
    "markdown-it": "^13.0.1"
  }
}
